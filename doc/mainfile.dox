/*! \mainpage sudoku++
* \section intro_sec Introduction
* This project implements a C++ template library for creating representing and solving sudoku boards. It depends on some of the facilities of C++11 so will
not compile with an earlier version of C++.
* \section install_sec Installation
* You only have to include sudoku.h to use it.
* \section example
  \subsection source_code
<small>
<code>
<pre style="line-height: 1.0">
    #include "sudoku.h"

    int
    main()
    {
        board<9> b( "000" "630" "005"
                    "504" "000" "900"
                    "800" "400" "000"

                    "120" "050" "300"
                    "308" "060" "701"
                    "006" "010" "029"

                    "000" "009" "004"
                    "003" "000" "207"
                    "200" "071" "000" );

        b.print_large();
        b.heuristic_solution();
        b.print_counts();
        b.print_large();
    }
</pre>
</code>
</small>
